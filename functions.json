{
  "functions": {
    "async": ["({ eval \"$@\"; } &) >/dev/null 2>&1"],
    "atom": ["eval \"function ${1}() echo ${1}\""],
    "c": ["pbcopy"],
    "cf": ["pbpaste|pbcopy"],
    "dirbak": ["cp -r \"${1}\" \"${1}.bak\""],
    "dirrest": ["cp -r \"${1}.bak\" \"${1}\" && \\", "rm -rf \"${1}.bak\""],
    "enum.onchar": ["echo \"${1}\" | sd \"(^\\s+|\\s+$)\" \" | sd \"\" \"\n\""],
    "enum.str": ["echo \"${1}\" | sd \" \" \"\\n\""],
    "filebak": ["cp \"${1}\"{,.bak}"],
    "filerest": ["cp \"${1}\"{.bak,} && rm \"${1}.bak\""],
    "global": [
      "unsetopt warncreateglobal",
      "eval \"${@}\"",
      "setopt warncreateglobal"
    ],
    "isstr": ["[[ \"${1}\" =~ [^0-9] ]]"],
    "loadf": [
      "ZSH_CONFIG_DIR=\"$HOME/zsh-config\"",
      "ZSH_BIN_DIR=\"$ZSH_CONFIG_DIR/bin\"",
      "eval \"$(${ZSH_BIN_DIR}/ruby/functions.rb get ${1})\""
    ],
    "loadf.list": [
      "ZSH_CONFIG_DIR=\"$HOME/zsh-config\"",
      "ZSH_BIN_DIR=\"$ZSH_CONFIG_DIR/bin\"",
      "${ZSH_BIN_DIR}/ruby/functions.rb list-all-functions"
    ],
    "nil": [
      "local name=\"$1\"",
      "local value=\"$(cat /dev/null)\"",
      "eval \"function $name() echo $value;\""
    ],
    "p": ["pbpaste"],
    "please": ["sudo !!"],
    "plux": ["chmod +x \"${1}\""],
    "reverse": ["perl -e 'print reverse <>'"],
    "rm.dsstore": ["find $(pwd) -name '*.DS_Store' -type f -ls -delete"],
    "rm.emptyfiles": ["find $(pwd) -type f -empty -print -delete"],
    "rm.emptydir": ["find $(pwd) -type d -empty -print -delete"],
    "safequote": [
      "local input=\"$1\"",
      "local quoted=\"\"",
      "",
      "# Escape special characters",
      "quoted=$(printf '%q' \"$input\")",
      "# Return the safely quoted string",
      "echo \"$quoted\""
    ],
    "symbol": ["use xxd; echo \"${1:-$RANDOM}\" | xxd -p"],
    "timestamp": ["date +'%Y-%m-%d %H:%M:%S'"],
    "with": [
      "# like `use` in `.zshrc`, except one line",
      "command -v \"${1}\" && true || { echo \"${1}\" not found; false; }"
    ]
  }
}
